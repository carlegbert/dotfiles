# cat this file
alias aliases='cat ~/.bash_aliases'

# laziness aliases
alias ..='_..' # see bash_scripts
alias ...='cd ../..'
alias ....='cd ../../..'
alias prj="cd ~/prj"
alias aptstuff='sudo apt-get update && sudo apt-get upgrade'

# xclip aliases
alias setclip='xclip -selection c'
alias getclip='xclip -selection clipboard -o'
